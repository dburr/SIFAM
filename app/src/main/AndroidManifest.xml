<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.caraxian.sifam">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <application
        android:name=".SIFAM"
        android:allowBackup="true"
        android:icon="@mipmap/launcher_maki"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ErrorDetected" />
        <activity android:name=".AppPreferences" />
        <activity android:name=".ImportAccounts" />
        <activity android:name=".ExportAccounts"></activity>

        <service android:name=".OverlayService" >
        </service>


    </application>

</manifest>
